{
  "name": "pokemoves",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "node src/build.js",
    "fetch": "node src/data/fetch.js",
    "install": "npm run fetch",
    "lint": "eslint . && prettier --write .",
    "start": "serve docs",
    "update": "npm run fetch && npm run build"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-css-order",
      "prettier-plugin-packagejson"
    ]
  },
  "dependencies": {
    "serve": "14.2.4"
  },
  "devDependencies": {
    "@eslint/js": "9.11.1",
    "eslint": "9.11.1",
    "eslint-config-prettier": "9.1.0",
    "globals": "15.10.0",
    "html-minifier-terser": "7.2.0",
    "nunjucks": "3.2.4",
    "prettier": "3.3.3",
    "prettier-plugin-css-order": "2.1.2",
    "prettier-plugin-packagejson": "2.5.6",
    "sass": "1.79.4",
    "uglify-js": "3.19.3"
  }
}
